thisconfigdir=../..
myfulldir=tests/gssapi
mydir=tests/gssapi
BUILDTOP=$(REL)..$(S)..
DEFINES = -DUSE_AUTOCONF_H
PROG_LIBPATH=-L$(TOPLIBD)
PROG_RPATH=$(KRB5_LIBDIR)

SRCS= $(srcdir)/t_imp_name.c $(srcdir)/s4utest.c

OBJS= t_imp_name.o s4utest.o

all:: t_imp_name s4utest

t_imp_name: t_imp_name.o $(GSS_DEPLIBS) $(KRB5_BASE_DEPLIBS)
	$(CC_LINK) -o t_imp_name t_imp_name.o $(GSS_LIBS) $(KRB5_BASE_LIBS)

s4utest: s4utest.o $(GSS_DEPLIBS) $(KRB5_BASE_DEPLIBS)
	$(CC_LINK) -o s4utest s4utest.o $(GSS_LIBS) $(KRB5_BASE_LIBS)

clean::
	$(RM) t_imp_name s4utest

