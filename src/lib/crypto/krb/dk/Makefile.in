mydir=lib/crypto/krb/dk
BUILDTOP=$(REL)..$(S)..$(S)..$(S)..
LOCALINCLUDES = -I$(srcdir)/..  -I$(srcdir)/../../@CRYPTO_IMPL@
DEFS=

##DOS##BUILDTOP = ..\..\..\..
##DOS##PREFIXDIR=dk
##DOS##OBJFILE=..\$(OUTPRE)dk.lst

PROG_LIBPATH=-L$(TOPLIBD)
PROG_RPATH=$(KRB5_LIBDIR)

STLIBOBJS=\
	checksum_hmac.o	\
	checksum_cmac.o	\
	checksum_gmac.o	\
	dk_aead.o	\
	dk_ccm.o	\
	dk_gcm.o	\
	derive.o	\
	stringtokey.o

OBJS=\
	$(OUTPRE)checksum_hmac.$(OBJEXT)\
	$(OUTPRE)checksum_cmac.$(OBJEXT)\
	$(OUTPRE)checksum_gmac.$(OBJEXT)\
	$(OUTPRE)dk_aead.$(OBJEXT)	\
	$(OUTPRE)dk_ccm.$(OBJEXT)	\
	$(OUTPRE)dk_gcm.$(OBJEXT)	\
	$(OUTPRE)derive.$(OBJEXT)	\
	$(OUTPRE)stringtokey.$(OBJEXT)

SRCS=\
	$(srcdir)/checksum_hmac.c	\
	$(srcdir)/checksum_cmac.c	\
	$(srcdir)/checksum_gmac.c	\
	$(srcdir)/dk_aead.c	\
	$(srcdir)/dk_ccm.c	\
	$(srcdir)/dk_gcm.c	\
	$(srcdir)/derive.c	\
	$(srcdir)/stringtokey.c

##DOS##LIBOBJS = $(OBJS)

all-unix:: all-libobjs

includes:: depend

depend:: $(SRCS)

clean-unix:: clean-libobjs

@libobj_frag@

